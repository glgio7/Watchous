{
	"version": 2,
	"builds": [
		{ "src": "../server/src/index.ts", "use": "@vercel/node" },
		{ "src": "./package.json", "use": "@vercel/static-build" }
	],
	"routes": [
		{ "handle": "filesystem" },
		{
			"src": "/api/(.*)",
			"dest": "../server/src/index.ts"
		},
		{
			"src": "/(.*)",
			"dest": "/"
		}
	]
}
