{
	"version": 2,
	"builds": [
		{
			"src": "client/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "client/build"
			}
		},
		{
			"src": "server/package.json",
			"use": "@vercel/node",
			"config": {
				"outputDirectory": "server/dist"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "server/dist/$1"
		},
		{
			"src": "/(.*)",
			"dest": "client/build/$1"
		}
	]
}
